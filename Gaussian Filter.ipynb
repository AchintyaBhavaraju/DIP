    "import cv2\n",
    "import numpy\n",
    "\n",
    "path = (r\"C:\\Users\\achin\\Downloads\\Gauss1.jpg\")\n",
    "img = cv2.imread(path)\n",
    "dst = cv2.GaussianBlur(img,(5,5),cv2.BORDER_DEFAULT) \n",
    "\n",
    "cv2.imshow('image', numpy.hstack((img, dst)))\n",
    "cv2.waitKey(0);\n",
    "cv2.destroyAllWindows();"
   ]
