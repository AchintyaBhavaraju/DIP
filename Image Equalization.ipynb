    "import numpy as np\n",
    "import cv2 as cv\n",
    "img = cv.imread ('C:/Users/achin/Downloads/helloworld.jpg')\n",
    "equ = cv.equalizeHist(img)\n",
    "res = np.hstack((img, equ))\n",
    "cv.imshow('img', res)\n",
    "cv.waitKey(0)\n",
    "cv.destroyAllWindows()\n",
    "\n"
